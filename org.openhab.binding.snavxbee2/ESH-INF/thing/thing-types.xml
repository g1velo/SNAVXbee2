	<!-- Bridge definition -->
	<bridge-type id="bridge">
		<label>SNAV Xbee2 Serial Gateway</label>
		<description>Xbee S2 connected using serial port
        </description>
		<config-description>
			<parameter name="SerialPortName" type="text" required="true">
				<context>Serial Port Name</context>
				<label>Serial Port Name</label>
				<description>Serial Port Name used to connect to XBee Coordinator</description>
			</parameter>
			<parameter name="SerialPortSpeed" type="text" required="true">
				<context>Serial Port Speed ( BaudRate )</context>
				<label>Serial Port Name</label>
				<description>Serial Port Name used to connect to XBee Coordinator</description>
			</parameter>
			<parameter name="ResetXBee" type="boolean" required="false">
				<context>ResetContext</context>
				<label>Send Reset() to this XBee Device</label>
				<description>In Case Thing is offline and should not try to set to true</description>
				<default>false</default>
			</parameter>
			<parameter name="WatchDogTimer" type="text" required="false">
				<context>WatchDogTimer</context>
				<label>WatchDog Timer</label>
				<description>WatchDogTimer will check local device is open</description>
				<default>false</default>
			</parameter>

		</config-description>
	</bridge-type>
